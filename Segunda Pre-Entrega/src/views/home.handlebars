<h1>Productos</h1>
{{#each products}}
<div>
    <h2>Titulo: {{this.title}}</h2>
    <h2>Descripcion: {{this.description}}</h2>
    <h2>Codigo: {{this.code}}</h2>
    <h2>Precio: {{this.price}}</h2>
    {{#if this.status}}
    <h2>Estado: Verdadero</h2>
    {{else}}
    <h2>Estado: Falso</h2>
    {{/if}}
    <h2>Stock: {{this.stock}}</h2>
    <h2>Category: {{this.category}}</h2>
    <img src="{{this.thumbnails}}" class="foto_producto">
</div>
{{/each}}

<script src="socket.io/socket.io.js"></script>
<link rel="stylesheet" href="/css/styles.css">